<body>
	<!-- banner-2 -->
	<div class="page-head_agile_info_w3l">

	</div>
	<!-- //banner-2 -->

	<!-- page -->
	<div class="services-breadcrumb">
		<div class="agile_inner_breadcrumb">
			<div class="container">
				<ul class="w3_short">
					<li>
						<a href="index.html">Home</a>
						<i>|</i>
					</li>
					<li>Electronics</li>
				</ul>
			</div>
		</div>
	</div>
	<!-- //page -->

	<!-- top Products -->
	<div class="ads-grid py-sm-5 py-4">
		<div class="container py-xl-4 py-lg-2">
			<!-- tittle heading -->
			<h3 class="tittle-w3l text-center mb-lg-5 mb-sm-4 mb-3">
				<span>Đ</span>iện thoại
				<span>&amp;</span>
				<span>M</span>áy tính</h3>
			<!-- //tittle heading -->

			<div class="row">
				<!-- product left -->
				<div class="agileinfo-ads-display col-lg-9">
					<div class="wrapper">
						<!-- first section -->
						<div class="product-sec1 px-sm-4 px-3 py-sm-5  py-3 mb-4">
							<div class="row">
								{{#each brandProducts}}
								<div class="col-md-4 product-men mt-md-0 mt-5">
									<div class="men-pro-item simpleCart_shelfItem">
										<div class="men-thumb-item text-center">
											<img src="{{this.imgURL}}" alt="">
											<div class="men-cart-pro">
												<div class="inner-men-cart-pro">
													<a href="/products/{{this.brand}}/{{this.id}}" class="link-product-add-cart">Xem nhanh</a>
												</div>
											</div>
											<span class="product-Mới-top">Mới</span>

										</div>
										<div class="item-info-product text-center border-top mt-4">
											<h4 class="pt-1">
												<a href="/products/{{this.brand}}/{{this.id}}">{{this.name}}</a>
											</h4>
											<div class="info-product-price my-2">
												<span class="item_price">${{this.currentPrice}}</span>
												<del>${{this.oldPrice}}</del>
											</div>
										</div>
									</div>
								</div>
								{{/each}}
							</div>
						</div>
						<!-- //first section -->
					</div>
				</div>
				<!-- //product left -->
			</div>
		</div>
	</div>
	<!-- //top products -->

	{{>middle-section}}

	<!-- js-files -->
	{{>jquery}}
	{{>nav-smooth-scroll}}
	{{>popup-modal}}
	{{>cart-js}}
	{{>password-script}}
	{{>smoothscroll}}

	<!-- for bootstrap working -->
	<script src="js/bootstrap.js"></script>
	<!-- //for bootstrap working -->
	<!-- //js-files -->

</body>